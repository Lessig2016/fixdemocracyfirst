<p id="notice"><%= notice %></p>

<h2>Listing Elections</h2>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>RepsWithUs ID</th>
      <th>Name</th>
      <th>State</th>
      <th>Office Type</th>
      <th>Special</th>
      <th>Election Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elections.each do |election| %>
      <tr>
        <td><%= election.rwu_id %></td>
        <td><%= election.name %></td>
        <td><%= election.state %></td>
        <td><%= election.office_type_id %></td>
        <td><%= election.special %></td>
        <td><%= election.election_year %></td>
        <td><%= link_to 'Show', election %></td>
        <td><%= link_to 'Edit', edit_election_path(election) %></td>
        <td><%= link_to 'Destroy', election, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Election', new_election_path, class: "btn btn-primary" %>
