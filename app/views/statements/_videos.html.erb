<div id="video-answers" class="row col-lg-12">
<% @videos.each do |video| %>  
  <div class="col-lg-4">
    <div style="padding: 0">
      <iframe class="col-lg-12" style="margin-bottom: 0" src="<%=video.youtube_embed_url%>?hd=1&rel=0&autohide=1&showinfo=0" frameborder="0"></iframe>
      <h4 class="col-lg-12">
        <%=link_to "#{video.title}", {controller: :statements, action: :show, id: video.id} %>
      </h4>
 
    </div>
  </div>
<% end %>
</div>